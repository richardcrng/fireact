{
  "name": "fireact",
  "version": "0.1.4",
  "description": "Hooks, middleware and helpers for using Firebase with React",
  "main": "dist/index.js",
  "jest": {
    "rootDir": "src"
  },
  "scripts": {
    "test": "jest --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcrng/fireact.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/richardcrng/fireact/issues"
  },
  "homepage": "https://github.com/richardcrng/fireact#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.1",
    "@testing-library/react-hooks": "^3.1.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "coveralls": "^3.0.7",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.11.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@types/jest": "^24.0.21",
    "@types/lodash": "^4.14.136",
    "@types/ramda": "^0.26.18",
    "firebase": "^7.2.3",
    "lodash": "^4.17.15",
    "ramda": "^0.26.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  }
}
